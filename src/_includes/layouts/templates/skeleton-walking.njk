<style>
    #zoomContainer {
        display: grid;
        place-items: center;
    }
    .walking-left g {
        animation-name: rotateAnimLeft;
        animation-iteration-count: 7;
        animation-direction: alternate reverse;
    }
    .walking-right g {
        animation-name: rotateAnimRight;
        animation-iteration-count: 7;
        animation-direction: alternate;
    }
    [class^=walking] g {
        fill: white;
    }
    .leg-container {
        max-height: 460px;
    }

    @keyframes rotateAnimLeft {
        0% { transform: rotate(25deg) translateY(0); }
        10% { transform: rotate(-25deg) translateY(-10px); }
        20% { transform: rotate(25deg) translateY(0); }
        40% { transform: rotate(-25deg) translateY(-10px); }
        60% { transform: rotate(25deg) translateY(0); }
        80% { transform: rotate(-25deg) translateY(-10px); }
        100% { transform: rotate(25deg) translateY(0); }
    }
    @keyframes rotateAnimRight {
        0% { transform: rotate(-25deg) translateY(-10px); }
        10% { transform: rotate(25deg) translateY(0); }
        20% { transform: rotate(-25deg) translateY(-10px); }
        40% { transform: rotate(25deg) translateY(0); }
        60% { transform: rotate(-25deg) translateY(-10px); }
        80% { transform: rotate(25deg) translateY(0); }
        100% { transform: rotate(-25deg) translateY(-10px); }
    }
</style>
<div id="zoomContainer" class="relative h-full overflow-hidden w-full">
    <svg stroke-width="5" class="absolute h-full leg-container w-full" id="leftSkeletonWalkingContainer" xmlns="http://www.w3.org/2000/svg" xml:space="preserve" viewBox="0 0 3796.06 6410.27">
    </svg>
    <svg stroke-width="7" class="absolute h-full leg-container w-full" id="rightSkeletonWalkingContainer" xmlns="http://www.w3.org/2000/svg" xml:space="preserve" viewBox="0 0 3796.06 6410.27">
