            <path fill="white" stroke="black" d="m1642.76 1675.86 32.02-1450.539 1664.81 96.695.04 1611.724-1696.87-257.88Z"/>
            <path fill="white" stroke="black" d="m1642.76 1675.86 32.02-1450.539 1664.81 96.695.04 1611.724-1696.87-257.88ZM1029.16 2045.1l310.38-62.12 569.13 72.47-331.06 113.82-548.45-124.17ZM1256.84 1827.81l1252.02 227.64M2788.24 1983.02 1598.3 1755.38"/>
